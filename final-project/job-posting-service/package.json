{
  "name": "codejam-cap-llm",
  "version": "1.0.0",
  "description": "A SAP Cloud Application Programming Model & AI example using the SAP Cloud SDK for AI.",
  "repository": "https://github.com/SAP-samples/codejam-cap-llm.git",
  "license": "UNLICENSED",
  "private": false,
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@cap-js/hana": "^2",
    "@cap-js/sqlite": "^2.0.3",
    "@sap-ai-sdk/langchain": "2.3.0",
    "@sap-ai-sdk/orchestration": "2.6.0",
    "@sap/cds": "^9.4.2",
    "langchain": "^1.1.3",
    "@langchain/classic": "^1.0.5"
  },
  "devDependencies": {
    "@sap/cds-dk": "^9.3.2",
    "eslint": "^9.37.0"
  },
  "scripts": {
    "build_production": "cds build --production",
    "start": "cds-serve",
    "watch_hybrid": "cds watch --profile hybrid --livereload false"
  },
  "cds": {
    "sql": {
      "native_hana_associations": false
    },
    "requires": {
      "[hybrid]": {
        "db": "hana"
      }
    }
  }
}
